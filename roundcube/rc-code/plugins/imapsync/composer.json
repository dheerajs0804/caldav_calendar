	"license" : "GPL",
	"authors" : [{
			"name" : "ServerGurus",
			"role" : "Developer",
			"homepage" : "https://servergurus.de"
		}, {
			"name" : "Github project contributors page",
			"homepage" : "https://github.com/server-gurus/RCimapSync/graphs/contributors"
		}
	],
	"repositories" : [{
			"type" : "composer",
			"url" : "https://plugins.roundcube.net"
		}
	],
	"require" : {
		"php": ">=5.3.0",
		"roundcube/plugin-installer" : ">=0.1.6"
	},
	"support" : {
